void drawAnimation(void) {
  static uint8_t frame = 0;          // Current frame index
  static unsigned long phaseStartTime = 0; // Start time of the current phase
  static bool isCase0 = true;        // Flag to track if we are in case 0

  // Get the current time
  unsigned long currentTime = millis();

  // Check if we need to switch phases
  if (currentTime - phaseStartTime >= DISPLAY_DURATION) {
    phaseStartTime = currentTime;  // Reset phase start time
    isCase0 = !isCase0;            // Toggle between case 0 and the animation loop
    frame = isCase0 ? 0 : 1;       // Reset frame index based on phase
  }

  u8g2.clearBuffer();

  if (isCase0) {
    // Case 0: Static display
    u8g2.setFont(u8g2_font_tenthinguys_tr);
    u8g2.drawStr(0, 10, "Love u Mangusta");
    u8g2.drawXBMP(0, 15, timon_width, timon_height, heart_bits[6]);
    u8g2.sendBuffer();
  } else {
    // Loop through other frames as fast as possible
    switch (frame) {
      case 1: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[0]); break;
      case 2: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[1]); break;
      case 3: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[2]); break;
      case 4: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[3]); break;
      case 5: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[4]); break;
      case 6: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[4]); break;
      case 7: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[4]); break;
      case 8: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[3]); break;
      case 9: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[2]); break;
      case 10: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[1]); break;
      case 11: u8g2.drawXBMP(0, 15, heart_width, heart_height, heart_bits[0]); break;
    }
    u8g2.sendBuffer();

    // Advance to the next frame
    frame++;
    if (frame > 11) {
      frame = 1; // Loop back to the first frame in the animation
    }
  }
}

// Function to toggle all LEDs
void toggleAllLEDs(bool state, int brightness) {
  // Loop through each pin in pwmPins array
  for (int i = 0; i < sizeof(pwmPins) / sizeof(pwmPins[0]); i++) {
    // If LEDs should be on, set the specified brightness
    if (state) {
      analogWrite(pwmPins[i], brightness);
    } else {
      analogWrite(pwmPins[i], 0); // Turn off LED
    }
  }
}


void setup() {
  // Set pin modes for buttons
  pinMode(BUTTON_PIN, INPUT_PULLUP);     // Toggle button with internal pull-up resistor
  pinMode(DIM_BUTTON_PIN, INPUT_PULLUP); // Dim button with internal pull-up resistor

  // Initialize PWM pins for LEDs
  for (int i = 0; i < sizeof(pwmPins) / sizeof(pwmPins[0]); i++) {
    pinMode(pwmPins[i], OUTPUT);      // Set each PWM pin as output
    analogWrite(pwmPins[i], 0);      // Start with all LEDs off
  }

  // Optionally, initialize the display (if you are using one)
  u8g2.begin();  // Initialize the OLED display (assuming you are using the U8G2 library)
}


void loop() {
  // Read button states
  int buttonReading = digitalRead(BUTTON_PIN);
  int dimButtonReading = digitalRead(DIM_BUTTON_PIN);

  // Handle the toggle button with debounce
  if (buttonReading != lastButtonState) {
    lastDebounceTime = millis(); // Update debounce timer
  }
  if ((millis() - lastDebounceTime) > debounceDelay) { // Check if debounce delay has passed
    if (buttonReading == LOW && !buttonPressed) { // Button was pressed
      ledState = !ledState; // Toggle LED state
      toggleAllLEDs(ledState, brightnessSteps[currentStepIndex]); // Apply the current brightness step
      buttonPressed = true; // Mark button as pressed
    } else if (buttonReading == HIGH) { 
      buttonPressed = false; // Reset flag when button is released
    }
  }

  // Handle the dimming button with debounce
  if (dimButtonReading != lastDimButtonState) {
    lastDimDebounceTime = millis(); // Update debounce timer for dimming button
  }
  if ((millis() - lastDimDebounceTime) > dimDebounceDelay) { // Check if debounce delay has passed
    if (dimButtonReading == LOW && !dimButtonPressed) { // Dim button pressed
      // Move to the next dimming step
      currentStepIndex++;
      if (currentStepIndex >= sizeof(brightnessSteps) / sizeof(brightnessSteps[0])) {
        currentStepIndex = 0; // Loop back to full brightness
      }

      // If LEDs are on, update brightness
      if (ledState) {
        toggleAllLEDs(true, brightnessSteps[currentStepIndex]); // Set LEDs to the new brightness
      }
      dimButtonPressed = true; // Mark dimming button as pressed
    } else if (dimButtonReading == HIGH) {
      dimButtonPressed = false; // Reset flag when dimming button is released
    }
  }

  // Update last button states for the next loop
  lastButtonState = buttonReading;
  lastDimButtonState = dimButtonReading;

  // Draw the animation on the display
  u8g2.firstPage();
  do {
    drawAnimation(); // Call the drawing function (make sure you have implemented this function)
  } while (u8g2.nextPage()); // Display the contents of the page
}
